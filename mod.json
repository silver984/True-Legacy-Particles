	"id": "silverstar.legacy-particles",
	"name": "True Legacy Particles",
	"version": "v1.2.0",
	"developer": "SILVERSTAR",
	"description": "",
	"links": {
		"source": "https://github.com/silver984/True-Legacy-Particles"
	},
	"tags": [
		"offline",
		"customization",
		"gameplay"
	],
	"settings": {
		"switch": {
			"name": "Turn TLP ON",
			"type": "bool",
			"default": true
		},
		"ground-particles": {
			"type": "title",
			"name": "Player particle system setting(s)"
		},
		"legacy-tracking": {
			"name": "Legacy Tracking",
			"type": "bool",
			"description": "Restores how ground particles followed the player.",
			"enable-if": "switch",
			"default": true
		},
		"legacy-scaling": {
			"name": "Legacy Scaling",
			"type": "bool",
			"description": "Restores how the player's particles were scaled down when you enter a mini portal.",
			"enable-if": "switch",
			"default": false
		},
		"inconst-values": {
			"name": "Inconsistent Values",
			"type": "bool",
			"description": "Restores how the player's particles change and shift in space, speed, and size after entering certain portals.",
			"enable-if": "switch",
			"default": true
		},
		"land-particles": {
			"type": "title",
			"name": "Player land particle setting(s)"
		},
		"no-rotation": {
			"name": "No Rotation",
			"type": "bool",
			"enable-if": "switch",
			"default": true
		},
		"dash-particles": {
			"type": "title",
			"name": "Dash particle setting(s)"
		},
		"center-dash": {
			"name": "Centered Dash Particles",
			"type": "bool",
			"enable-if": "switch",
			"default": true
		},
		"center-dash-x": {
			"name": "x axis",
			"type": "bool",
			"description": "Center the x axis.",
			"enable-if": "center-dash && switch",
			"default": true
		},
		"center-dash-y": {
			"name": "y axis",
			"type": "bool",
			"description": "Center the y axis.",
			"enable-if": "center-dash && switch",
			"default": true
		},
		"misc": {
			"type": "title",
			"name": "Miscellaneous"
		},
		"player-1": {
			"name": "Player 1",
			"type": "bool",
			"enable-if": "switch",
			"default": true
		},
		"player-2": {
			"name": "Player 2",
			"type": "bool",
			"enable-if": "switch",
			"default": true
		}
	}
}